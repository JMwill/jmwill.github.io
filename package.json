{
  "name": "jmwill.github.io",
  "version": "0.0.2",
  "description": "githug main page",
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write src",
    "rollup:watch": "rollup -w --config",
    "start": "http-server",
    "prebuild": "npm run clean",
    "build": "MY_BUILD_ENV=prod rollup --config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JMwill/jmwill.github.io.git"
  },
  "keywords": [
    "github",
    "main-page"
  ],
  "author": "JMwill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JMwill/jmwill.github.io/issues"
  },
  "homepage": "https://github.com/JMwill/jmwill.github.io#readme",
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "http-server": "^0.12.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.11"
  },
  "dependencies": {},
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2021
    },
    "plugins": [
      "prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "assets/**/*.{json,css,md}": [
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ],
    "src/**/*.js": [
      "eslint --fix"
    ]
  }
}
